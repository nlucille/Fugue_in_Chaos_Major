  import random
from weasyprint import HTML, CSS
from weasyprint.fonts import FontConfiguration


novel="""

#Fugue in Chaos major
###a novel by naomi jones

"""

for chapter in range(1,51):
  novel+= f"""

##Chapter {chapter}

"""

  for w in text:
    if w is "A":
      novel+=" "
      novel+=random.choice(nouns)

    if w is "B":
      novel+=" "
      novel+=random.choice(verbs)

    if w is "C":
      novel+=" "
      novel+=random.choice(common)

    if w is "D":
      novel+=" "
      novel+=random.choice(adjectives)

    if w is "E":
      novel+=" "
      novel+=random.choice(adverbs)

    if w is "F":
      novel+=" "
      novel+=random.choice(Shakespeare)

    if w is "G":
      novel+=" "
      novel+=random.choice(resume)
    
    if w is "^":
      novel+=" "
      novel+=random.choice(proverbs)

    if w is "_":
      novel+=" "
      novel+=random.choice(strange)

    if w is ",":
      novel+="."

    if w is "|":
      novel+=".\n\n"

    if w is "3":
      novel+=" "
      novel+=random.choice(inter)

    if w is "4":
      novel+=" "
      novel+=random.choice(pronouns)

    if w is "2":
      novel+=" "
      novel+=random.choice(resume)

    if w is "5":
      novel+=" "
      novel+=random.choice(time)

    if w is "6":
      novel+=" "
      novel+=random.choice(encouraging)

    if w is "7":
      novel+=" "
      novel+=random.choice(stop)

    if w is "/":
      novel+=" "
      novel+=random.choice(verbs2)

  #print(novel)


html=markdown.markdown(novel)

# prepare WeasyPrint
font_config = FontConfiguration()
rendered_html = HTML(string=html)

css = CSS(string='''
@import url('https://fonts.googleapis.com/css2?family=Merriweather&display=swap');

@import url('https://fonts.googleapis.com/css2?family=Inconsolata:wght@300&display=swap');
body {
font-family: 'Inconsolata', serif;
}
hr {
  break-after: recto; 
}

h1 {
  font-size: 50pt;
  text-align:center;
  margin-top: 3in;
  font-family: 'Merriweather',cursive;
}
h2{
  break-before: recto;
  font-family: 'Merriweather',cursive;
}

h3 {
  font-size: 20pt;
  text-align:center;
}

/* set the basic page geometry and start the incrementer */
@page {
  font-family: 'Inconsolata', serif;
  margin: 1in;
  size: letter;
  counter-increment: page;
  @bottom-center {
    content: "Fugue in Chaos Minor";
    text-align:center;
    font-style: italic;
    color: #666666;
  }
}

/* print the page number on the bottom-right of recto pages */
@page :right {
  @bottom-right{
    content: "[" counter(page) "]";
    text-align:right;
    color: #666666;
    visibility: invisible;
  }
}

/* print the page number on the bottom-left of verso pages */
@page :left {
  @bottom-left{
    content: "[" counter(page) "]";
    text-align:left;
    color: #666666;
  }
}

/* blank the footer on the first page */
@page:first{
  @bottom-left {content: ""}
  @bottom-right {content: ""}
  @bottom-center {content: ""}
}


''', font_config=font_config)

# This creates a PDF with all the above settings
rendered_html.write_pdf('/content/fugue.pdf', stylesheets=[css],font_config=font_config)
